
{
    class Program
    {
        static void Main(string[] args)
        {
            SayHi("Mike", 12);
            SayHi("Peter", 15);
            SayHi("Mary", 20);
            Console.ReadLine();
        }

        static void SayHi(string name, int age)
        {
            Console.WriteLine("Hello " + name + " you are " + age);
        }
    }
}


